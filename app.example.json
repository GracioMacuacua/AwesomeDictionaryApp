        "backgroundColor": "BACKGROUND_COLOR"
      },
      "package": "ANDROID_PACKAGE",
      "edgeToEdgeEnabled": "EDGE_TO_EDGE_ENABLED"
    },
    "web": {
      "favicon": "FAVICON_PATH"
    },
    "extra": {
      "eas": {
        "projectId": "PROJECT_ID"
      }
    },
    "owner": "graciomacuacua",
    "plugins": [
      "expo-router",
      "expo-font",
      [
        "expo-sqlite",
